{
  "name": "mayaaalokam-roleplay",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "bot:dev": "tsx backend/server.ts",
    "bot:build": "tsc -p backend/tsconfig.json && npx copyfiles -u 1 backend/*.js backend/dist",
    "bot:start": "node backend/dist/server.js",
    "post-duty-panel": "node backend/post-duty-panel-separate.js",
    "remove-duty-panel": "node backend/remove-duty-panel.js",
    "dev:all": "concurrently \"npm run dev\" \"npm run bot:dev\"",
    "start:all": "concurrently \"npm start\" \"npm run bot:start\"",
    "deploy:frontend": "npm run build && npm start",
    "deploy:backend": "npm run bot:build && npm run bot:start",
    "postinstall": "npm run bot:build",
    "test": "npm run lint",
    "deploy:check": "node deploy.js",
    "prepare": "npm run bot:build",
    "render:build": "npm ci && npm run build",
    "render:start": "npm start",
    "diagnostic": "node backend/diagnostic.js",
    "oauth-diagnostic": "node backend/discord-oauth-diagnostic.js"
  },
  "dependencies": {
    "@react-three/drei": "^9.109.2",
    "@react-three/fiber": "^8.17.10",
    "@react-three/rapier": "^1.4.0",
    "@supabase/supabase-js": "^2.50.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.18.1",
    "autoprefixer": "^10.4.16",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "discord.js": "^14.19.3",
    "dotenv": "^16.5.0",
    "eslint": "^8.55.0",
    "eslint-config-next": "14.0.4",
    "express": "^5.1.0",
    "framer-motion": "^10.16.16",
    "lucide-react": "^0.298.0",
    "meshline": "^3.3.1",
    "mysql2": "^3.14.1",
    "next": "14.2.30",
    "next-themes": "^0.2.1",
    "node-cron": "^3.0.3",
    "postcss": "^8.4.32",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.48.2",
    "react-icons": "^4.12.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^3.3.6",
    "three": "^0.177.0",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "@types/node-cron": "^3.0.11",
    "@types/react": "^18.2.42",
    "@types/react-dom": "^18.2.17",
    "concurrently": "^8.2.2",
    "copyfiles": "^2.4.1",
    "tsx": "^4.19.4",
    "typescript": "^5.3.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
