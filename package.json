  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:github-pages": "next build",
    "start": "next start",
    "lint": "next lint",
    "bot:dev": "tsx backend/server.ts",
    "bot:build": "tsc backend/server.ts --outDir backend/dist --target es2020 --moduleResolution node --allowSyntheticDefaultImports --esModuleInterop",
